<div id="home">
  <div class="container">
    <img class="logo" src="assets/images/LandingPageGraphics/onTrackLogo.png" alt="OnTrack logo">
    <img class="lines" src="assets/images/LandingPageGraphics/Landing_page.png" alt="transparent lines">
    <img class="logo2" src="assets/images/LandingPageGraphics/logo2.png" alt="logo2">
    <h4>Welcome back! Please login to your account.</h4>

    <div class="row">
      <div class="col-8 offset-2">
        <h1>{{msg}}</h1>
        
       <form>
  <div class="form-group">
    <label for="exampleInputEmail1"></label>
    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
    <small id="emailHelp" class="form-text text-muted"></small>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1"></label>
    <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
  </div>
  <div class="form-group form-check">
    <input type="checkbox" class="form-check-input" id="exampleCheck1">
    <label class="form-check-label" for="exampleCheck1">Remember me</label>
    
  </div>
  <button type="button" class="btn btn-primary btn-lg-Login">Login</button>
  <button type="button" class="btn btn-primary btn-lg-SignUp">Sign Up</button>
</form>
        
        <ul id="example-list" class="list-group">
          {{#each examples}}
          <li data-id="{{this.id}}" class="list-group-item">
            <a href="example/{{this.id}}">{{this.text}}</a>
            <button class="btn btn-danger float-right delete">ï½˜</button>
          </li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>